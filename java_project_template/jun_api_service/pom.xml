	<!-- 依赖声明 -->
    <dependencyManagement>
        <dependencies>
        <!-- SpringBoot的依赖配置-->
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-dependencies</artifactId>
