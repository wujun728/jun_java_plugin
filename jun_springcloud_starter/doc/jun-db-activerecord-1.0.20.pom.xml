        </developer>
    </developers>

    <scm>
        <url>https://github.com/wujun728/jun-spring-boot-starter/jun-db-activerecord.git</url>
        <connection>scm:git:github.com/wujun728/jun-spring-boot-starter/jun-db-activerecord.git</connection>
